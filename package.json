{
  "name": "map-obj",
  "version": "2.0.0",
  "description": "Map object keys and values into a new object",
  "license": "MIT",
  "repository": "sindresorhus/map-obj",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "sindresorhus.com"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
